{
    "tasks":
    [
        {
            "type": "shell",
            "label": "linux_pre_compile",
            "command": "cp",
            "args": [
                "-rf",
                "../_BuildSettings_LinuxGcc/Sdk.inl",
                "../BuildSetting.inl"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
        {
            "type": "shell",
            "label": "linux_compile_sdk",
            "command": "g++-10",
            "args": [
                "-std=c++2a",
                "-pthread",
                "-fcoroutines",
                "-c",
                "./Sdk/Environment/EnvironmentLinuxGcc/Implementation/*",
            ],
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "dependsOn": [
                "linux_pre_compile"
            ]
        },
        {
            "type": "shell",
            "label": "linux_compile_ut",
            "command": "g++-10",
            "args": [
                "-std=c++2a",
                "-pthread",
                "-fcoroutines",
                "-c",
                "./EntryPoint/EntryPoint.cpp",
            ],
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "dependsOn": [
                "linux_compile_sdk"
            ]
        },
        {
            "type": "shell",
            "label": "linux_compile_complete_echo",
            "command": "echo success",
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "dependsOn": [
                "linux_compile_ut"
            ]
        },
        {
            "type": "shell",
            "label": "linux_all_process",
            "command": "rm",
            "args": [
                "./*.o",
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "dependsOn": [
                "linux_compile_complete_echo"
            ]
        }
    ],
    "version": "2.0.0"
}
